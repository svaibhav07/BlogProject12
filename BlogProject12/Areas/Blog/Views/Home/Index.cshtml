@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model IEnumerable<BlogProject12.Models.BlogModel>
@{
    ViewData["Title"] = "Home Page";

}

<div class="row pb-3 backgroundWhite">

    @foreach (var blog in Model)
    {
        <div class="container">
            <div class="row">
                <h1>@blog.BlogTitle</h1>
                <a class="btn btn-primary" asp-action="Detail" asp-route-id ="@blog.Id"><h1>@blog.BlogTitle</h1></a>
                <br>
                <br>
                <br>
                <br>
            </div>
        </div>
    }
</div>
