@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@model IEnumerable<BlogProject12.Models.BlogModel>
@{
    ViewData["Title"] = "Home Page";

}

<div class="row pb-3 backgroundWhite">

    @foreach (var blog in Model)
    {
        <div class="container">
            <div class="row">
                
                <a class="btn btn-primary" asp-action="Detail" asp-route-id ="@blog.Id"><h3>@blog.BlogTitle</h3></a>
                
            </div>
        </div>
    }
</div>
